{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Seed Run Dev",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/",
      "args": [
        "--host=0.0.0.0:8080",
        "--profile=./test/app",
        "--release=false"
      ],
      "output": "${workspaceRoot}/build/seed",
      "trace": "verbose",
      "showLog": true,
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Seed Run Debug",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/",
      "args": ["--host=0.0.0.0:8080", "--profile=./test/app", "--debug"],
      "output": "${workspaceRoot}/build/seed",
      "trace": "verbose",
      "showLog": true,
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Seed Help",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/",
      "args": ["--help"],
      "output": "${workspaceRoot}/build/seed",
      "trace": "verbose",
      "showLog": true,
      "cwd": "${workspaceRoot}"
    }
  ]
}
